{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataEngineeringDummyDataFactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0304-034718-5xsv8ney"
		},
		"AzureDatabricksKeyVault_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0304-034718-5xsv8ney"
		},
		"is_key_vaults_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dataengineerin2555767360.vault.azure.net/"
		},
		"trigger_daily_properties_extract_daily_parameters_databricks_mode": {
			"type": "int",
			"defaultValue": 0
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataEngineeringDummyDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"is_key_vaults_properties_typeProperties_baseUrl": {
						"value": "[parameters('is_key_vaults_properties_typeProperties_baseUrl')]"
					},
					"AzureDatabricksKeyVault_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricksKeyVault_properties_typeProperties_existingClusterId')]"
					},
					"trigger_daily_properties_extract_daily_parameters_databricks_mode": {
						"value": "[parameters('trigger_daily_properties_extract_daily_parameters_databricks_mode')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}